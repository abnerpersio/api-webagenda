  "description": "API",
  "main": "dist/index.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "babel src --out-dir dist",
    "start": "npm run build && node ./dist/index.js",
    "dev": "nodemon --exec babel-node src/index",
