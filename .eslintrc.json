{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true,
      "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-underscore-dangle": "off",
      "class-methods-use-this": "off",
      "no-useless-escape": "off",
      "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
      "max-len": "off",
      "import/prefer-default-export": "off"
    }
}
